pipeline{
    agent any
    stages{
        stage("A"){
            steps{
                echo "========executing A========"
            }
        }
    stage("B"){
        steps{
            echo "========executing B========"
        }
        post{
            always{
                echo "========always========"
            }
            success{
                echo "========B executed successfully========"
            }
            failure{
                echo "========B execution failed========"
            }
        }
    }
    stage("c"){
        steps{
            echo "this is step c"
        }
    }
    }
}
